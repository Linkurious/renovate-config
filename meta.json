{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePatterns": ["(^|\\/)traefik$"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/containous/traefik"
    },
    {
      "matchPackageNames": ["jenkins/inbound-agent"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/jenkinsci/docker-agent/releases"
    },
    {
      "matchPackageNames": ["jenkins/jenkins"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://jenkins.io/changelog"
    }

  ]
}
